
@{
    ViewBag.Title = "MVC01 Razor Syntax";
    string adi = "Ali";
    string[] isimler = new string[] { "Murat", "Mesut", "Yılmaz" };
}

<h2>MVC01 Razor Syntax</h2>

<b>@ViewBag.Title</b>

@ViewBag.Title

<h3>Adi değişkeninin değeri : @adi</h3>


<h3>İsimleri Ekrana Yazdırma</h3>

@foreach (var item in isimler)
{
    <h3>@item</h3>
    @item
}

